#:kivy 1.10

#:import Styles styles.Styles

<CodenamesTableWidget>:
    canvas.before:
        Color:
            rgba: root.background_color
        Rectangle:
            pos: self.pos
            size: self.size
        Color:
            rgba: Styles.BORDER
        Rectangle:
            pos: (self.x + 9, self.y + 9)
            size: (self.width - 18, self.height - 18)
